{
    "language"        : "PYTHON",
    "experiment-name" : "food_classification_slp.1",
    "polling-time"    : 1,
    "resources" : {
        "my-machine" : {
            "scheduler"         : "managed",
            "max-concurrent"    : 2,
            "gpus"              : 2
        }
    },
    "tasks": {
        "cifar10" : {
            "type"       : "OBJECTIVE",
            "likelihood" : "GAUSSIAN",
            "main-file"  : "run_slp_base",
            "resources"  : ["my-machine"]
        }
    },
    "variables": {
        "max_norm_y" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : 0.1,
            "max"  : 6
        },
        "l_ir_y" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : -6,
            "max"  : -0.1
        },
        "log_init_learning_rate" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : -7,
            "max"  : -0.7
        }
    }
}
