{
    "language"        : "PYTHON",
    "experiment-name" : "food_classification_exp1.2",
    "polling-time"    : 1,
    "resources" : {
        "my-machine" : {
            "scheduler"         : "managed",
            "max-concurrent"    : 2,
            "gpus"              : 2
        }
    },
    "tasks": {
        "cifar10" : {
            "type"       : "OBJECTIVE",
            "likelihood" : "GAUSSIAN",
            "main-file"  : "run_cnn_base",
            "resources"  : ["my-machine"]
        }
    },
    "variables": {
        "kernel_config": {
            "type" : "ENUM",
            "size" : 1,
            "options" : ["a", "b"]
        },
        "max_norm_h2" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : 0.3,
            "max"  : 2.5
        },
        "l_ir_h2" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : -3,
            "max"  : 0.5
        },
        "max_norm_h3" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : 1,
            "max"  : 6
        },
        "l_ir_h3" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : -3,
            "max"  : 0.5
        },
        "max_norm_y" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : 0.2,
            "max"  : 2.5
        },
        "l_ir_y" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : -7,
            "max"  : -2
        },
        "log_init_learning_rate" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : -6.5,
            "max"  : -3
        }
    }
}
