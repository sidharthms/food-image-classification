{
    "language"        : "PYTHON",
    "experiment-name" : "food_classification_mlp3.1",
    "polling-time"    : 1,
    "resources" : {
        "my-machine" : {
            "scheduler"         : "managed",
            "max-concurrent"    : 2,
            "gpus"              : 2
        }
    },
    "tasks": {
        "cifar10" : {
            "type"       : "OBJECTIVE",
            "likelihood" : "GAUSSIAN",
            "main-file"  : "run_mlp_base",
            "resources"  : ["my-machine"]
        }
    },
    "variables": {
        "dim_h1" : {
            "type" : "INT",
            "size" : 1,
            "min"  : 4000,
            "max"  : 10000
        },
        "dim_h2" : {
            "type" : "INT",
            "size" : 1,
            "min"  : 300,
            "max"  : 2000
        },
        "dim_h3" : {
            "type" : "INT",
            "size" : 1,
            "min"  : 100,
            "max"  : 1000
        },
        "max_norm_h1" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : 0.3,
            "max"  : 2.5
        },
        "l_ir_h1" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : -5,
            "max"  : 0.5
        },
        "max_norm_h2" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : 1,
            "max"  : 10
        },
        "l_ir_h2" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : -3,
            "max"  : 1.5
        },
        "max_norm_h3" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : 1,
            "max"  : 8
        },
        "l_ir_h3" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : -3,
            "max"  : 0.5
        },
        "max_norm_y" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : 0.1,
            "max"  : 6
        },
        "l_ir_y" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : -6,
            "max"  : 0.5
        },
        "log_init_learning_rate" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : -7,
            "max"  : -0.7
        }
    }
}
